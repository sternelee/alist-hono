{
  "name": "alist-hono",
  "version": "0.0.1",
  "description": "use Hono and cloudflare to deploy alist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sternelee/alist-hono.git"
  },
  "keywords": [
    "alist",
    "hono",
    "cloudflare",
    "auth"
  ],
  "author": "sternelee <sternelee@gmail.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "SECURITY.md",
    "LICENSE",
    "docs"
  ],
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json}\"",
    "generate": "drizzle-kit generate --dialect=sqlite --schema=./src/db/schema --out=./migrations",
    "up": "wrangler d1 migrations apply alist --local",
    "up:prod": "wrangler d1 migrations apply alist",
    "deploy": "wrangler deploy --minify src/index.ts",
    "test": "ts-jest"
  },
  "dependencies": {
    "@types/blueimp-md5": "^2.18.2",
    "async-retry": "^1.3.3",
    "blueimp-md5": "^2.19.0",
    "hono": "^4.5.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240815.0",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@types/async-retry": "^1.4.8",
    "@types/jest": "^29.5.12",
    "@types/node": "22.4.1",
    "@types/qs": "^6.9.15",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.24.0",
    "drizzle-orm": "^0.33.0",
    "jest": "^29.7.0",
    "lucia": "^2.7.4",
    "prettier": "3.3.3",
    "qs": "^6.13.0",
    "ts-jest": "^29.2.4",
    "typescript-eslint": "^8.2.0",
    "uuid": "^10.0.0",
    "wrangler": "^3.72.0"
  },
  "type": "module"
}
