{
  "name": "alist-hono",
  "version": "0.0.1",
  "description": "use Hono and cloudflare to deploy alist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sternelee/alist-hono.git"
  },
  "keywords": [
    "alist",
    "hono",
    "cloudflare"
  ],
  "author": "sternelee <sternelee@gmail.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json}\"",
    "generate": "drizzle-kit generate --dialect=sqlite --schema=./src/db/schema --out=./migrations",
    "up": "wrangler d1 migrations apply alist --local",
    "up:prod": "wrangler d1 migrations apply alist",
    "deploy": "wrangler deploy --minify src/index.ts",
    "dev:unocss": "unocss --watch",
    "test": "ts-jest"
  },
  "dependencies": {
    "@hono/valibot-validator": "^0.5.1",
    "@libsql/kysely-libsql": "^0.4.1",
    "@unscatty/unocss-preset-daisy": "^1.0.0",
    "async-retry": "^1.3.3",
    "bencode": "^4.0.0",
    "better-auth": "^1.0.10",
    "blueimp-md5": "^2.19.0",
    "crypto-js": "^4.2.0",
    "daisy": "^0.2.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.28.1",
    "drizzle-orm": "^0.36.4",
    "hono": "^4.6.11",
    "qs": "^6.13.1",
    "uuid": "^11.0.3",
    "valibot": "1.0.0-beta.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@types/async-retry": "^1.4.9",
    "@types/bencode": "^2.0.4",
    "@types/blueimp-md5": "^2.18.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "22.9.3",
    "@types/qs": "^6.9.17",
    "@types/uuid": "^10.0.0",
    "@unocss/cli": "^0.65.1",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "^29.2.5",
    "typescript-eslint": "^8.15.0",
    "unocss": "^0.65.1",
    "wrangler": "^3.90.0"
  },
  "type": "module"
}
